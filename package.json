{
  "name": "tnff-gatsby",
  "version": "1.0.0",
  "description": "Website for Toronto Nepali Film Festival",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "npx eslint . --ext .js,.jsx",
    "lint-report-json": "npx eslint . --ext .js,.jsx -o .reports/eslint-report.json -f json src",
    "lint-report-html": "npx eslint --ext .js,.jsx -o .reports/eslint-report.html -f html",
    "lint-fix": "npx eslint . --fix --ext .js,.jsx",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toronto-nepali-film-festival/tnff-gatsby.git"
  },
  "author": "Sonal Ranjit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toronto-nepali-film-festival/tnff-gatsby/issues"
  },
  "homepage": "https://github.com/toronto-nepali-film-festival/tnff-gatsby#readme",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^9.2.0",
    "gatsby": "^4.4.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-offline": "^5.4.0",
    "gatsby-plugin-page-creator": "^4.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-sass": "^5.4.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-plugin-typography": "^4.4.0",
    "gatsby-remark-images-contentful": "^5.4.0",
    "gatsby-source-contentful": "^7.2.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-source-graphql": "^4.4.0",
    "gatsby-transformer-json": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}