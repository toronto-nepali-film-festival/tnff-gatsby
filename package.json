{
  "name": "tnff-gatsby",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_ROUTING_APIS=1 gatsby build",
    "develop": "GATSBY_EXPERIMENTAL_ROUTING_APIS=1 gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "npx eslint . --ext .js,.jsx",
    "lint-report-json": "npx eslint . --ext .js,.jsx -o .reports/eslint-report.json -f json src",
    "lint-report-html": "npx eslint . --ext .js,jsx -o .reports/eslint-report.html -f html",
    "lint-fix": "npx eslint . --fix --ext .js,.jsx",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^6.6.0",
    "gatsby": "^3.0.4",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-page-creator": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sass": "^4.0.2",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-plugin-typography": "^2.9.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-source-graphql": "^3.0.0",
    "gatsby-transformer-json": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "node": "^14.16.0",
    "node-sass": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "2.2.1",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
